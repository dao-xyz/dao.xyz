{
  "name": "dao.xyz",
  "version": "0.0.1",
  "author": "dao.xyz",
  "repository": "https://github.com/marcus-pousette",
  "license": "Apache-2.0",
  "type": "module",
  "sideEffects": false,
  "bin": {
    "daoxyz": "./lib/esm/bin.js"
  },
  "private": "false",
  "publishConfig": {
    "access": "public"
  },
  "module": "lib/esm/index.mjs",
  "types": "lib/esm/index.d.ts",
  "exports": {
    "import": "./lib/esm/index.mjs"
  },
  "files": [
    "lib",
    "src",
    "src/nginx-template.conf",
    "!src/**/__tests__",
    "!lib/**/__tests__",
    "LICENSE"
  ],
  "engines": {
    "node": ">=16.0"
  },
  "scripts": {
    "clean": "shx rm -rf lib/*",
    "build": "yarn clean && tsc -p tsconfig.json; tsc-esm -p tsconfig.json",
    "postbuild": "echo '{\"type\":\"module\"} ' | node ../../node_modules/.bin/json > lib/esm/package.json",
    "test": "node ../../node_modules/.bin/jest test  -c  ../../jest.config.ts --runInBand --forceExit",
    "test:unit": "node ../../node_modules/.bin/jest test  -c  ../../jest.config.unit.ts --runInBand --forceExit",
    "test:integration": "node ../node_modules/.bin/jest test -c  ../../jest.config.integration.ts --runInBand --forceExit"
  },
  "devDependencies": {
    "@dao-xyz/peerbit-test-utils": "^0.0.12",
    "@dao-xyz/peerbit-time": "^0.0.17",
    "dotenv": "^16.0.3",
    "go-ipfs": "^0.16.0",
    "ipfs": "^0.65.0",
    "ipfsd-ctl": "^12.2.2",
    "yargs": "^17.6.0"
  },
  "dependencies": {
    "@dao-xyz/peerbit": "^0.0.40",
    "@dao-xyz/peerbit-document": "^0.0.111",
    "@dao-xyz/peerbit-dynamic-access-controller": "^0.0.66",
    "@dao-xyz/peerbit-network-discovery": "^0.0.37",
    "@dao-xyz/peerbit-node": "^0.0.30",
    "@dao-xyz/peerbit-string": "^0.0.100",
    "buffer": "^6.0.3"
  }
}